<% init = @app[:databag][:init] %>

export SESSION_DATAKEY=<%=@app[:uniq][1]%>
export PUBLIC_BASE_URI=https://<%=@app[:hostname]%>
export CLOUDOS_DNS_DB_PASS=<%=@app[:dbpass]%>
export CLOUDOS_DNS_SERVER_PORT=<%=@app[:port]%>

<% if init['dyndns'] %>
export DNS_DYN=true
export DYNDNS_ACCOUNT=<%=init['dyndns']['account']%>
export DYNDNS_USER=<%=init['dyndns']['user']%>
export DYNDNS_PASSWORD=<%=init['dyndns']['password']%>
export DYNDNS_ZONE=<%=init['dyndns']['zone']%>
<% end %>

<% if init['djbdns'] %>
export DNS_DJB=true
<% end %>
