---
user: <%=@app[:run_as]%>
gitlab_url: https://<%=@app[:name]%>-<%=@app[:hostname]%>/
http_settings:
  self_signed_cert: false
repos_path: <%=@app[:run_as_home]%>/repositories/
auth_file: <%=@app[:run_as_home]%>/.ssh/authorized_keys

redis:
  bin: /usr/bin/redis-cli
  host: localhost
  port: 6379
  namespace: resque:gitlab
log_level: INFO
audit_usernames: false
